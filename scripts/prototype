echo "proto <name>: Compile <name>.adl with most-recent Ampersand prototype and install in ~/Sites/Ampersand/<name>"
$HOME/svn/Prototype/dist/build/prototype/prototype --version
if [ $# -ne 0 ]; then
if [ ! -s $1 ]; then # check is necessary since otherwise ampersand creates directory even if the filename was incorrect
echo File not found error: $1
exit 1
else
noExtension=${1%\.*}  # is filename without extension
echo prototype -p$HOME/www/$noExtension $1 --verbose $2 $3 $4 $5 $6 $7 $8 $9
$HOME/svn/Prototype/dist/build/prototype/prototype -p$HOME/www/$noExtension $1 --sqlPwd=anaxagoras0 --sqlLogin=sentinel --verbose $2 $3 $4 $5 $6 $7 $8 $9
# for a sql password: --sqlPwd=<password>
fi
fi
